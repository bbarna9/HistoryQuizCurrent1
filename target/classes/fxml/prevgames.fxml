<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PreviusGamesController">
   <children>
       <TableView fx:id="prevGamesTable" editable="true" layoutX="25.0" layoutY="55.0" prefHeight="300.0" prefWidth="700.0">
